  <connectionStrings>
    <!--<add name="DefaultConnection" connectionString="Server=(local);Database=TekConfAuth;User Id=sa;Password=Xz4*b.17/6;" providerName="System.Data.SqlClient" />-->
    <add name="DefaultConnection" connectionString="Server=(local);Database=TekConfAuth;Integrated Security=true" providerName="System.Data.SqlClient" />
    <add name="StorageConnection" connectionString="UseDevelopmentStorage=true" />
    <add name="MongoServer" connectionString="mongodb://localhost/conferences" />
    <add name="elmah-mongodb" connectionString="mongodb://localhost/elmah" />
  </connectionStrings>
  <appSettings>
    <add key="owin:appStartup" value="WebStartup" />
    <add key="webpages:Version" value="2.0.0.0" />
    <add key="webpages:Enabled" value="false" />
    <add key="PreserveLoginUrl" value="true" />
    <add key="ClientValidationEnabled" value="true" />
    <add key="UnobtrusiveJavaScriptEnabled" value="true" />
    <add key="ImageUrl" value="http://127.0.0.1:10000/devstoreaccount1/images/conferences/" />
    <add key="BaseUrl" value="http://localhost:25825/" />
    <!--<add key="BaseUrl" value="http://api.tekconf.com/" />-->
    <add key="webUrl" value="http://localhost:2118" />
    <add key="mongoServerAddress" value="localhost" />
    <add key="enableSimpleMembership" value="true" />
  </appSettings>
  <!--
